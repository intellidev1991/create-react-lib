{
  "name": "create-react-xlib",
  "description": "suppose this npm package name is 'my-lib'",
  "private": true,
  "version": "0.0.10",
  "scripts": {
    "dev": "vite serve docs",
    "doc": "rimraf docs/dist && vite build docs && serve -s docs/dist",
    "ssr": "rimraf docs/dist && vite-pages ssr docs && serve docs/dist",
    "build": "tsc && vite build",
    "prepublish": "rimraf ./dist && npm run build",
    "postinstall": "is-ci || husky install",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "is-ci": "^3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/node": "^17.0.42",
    "@types/react": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.0.1",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.56.1",
    "serve": "^14.0.1",
    "tailwindcss": "^3.2.3",
    "typed-css-modules": "^0.7.2",
    "typed-scss-modules": "^7.0.1",
    "vite": "^3.0.7",
    "vite-pages-theme-doc": "^3.1.4",
    "vite-plugin-dts": "^1.7.0",
    "vite-plugin-linter": "^1.2.0",
    "vite-plugin-mdx": "^3.5.6",
    "vite-plugin-react-pages": "^3.1.8",
    "vite-tsconfig-paths": "^3.5.2"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.es.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ]
}
